    "fib=[1,2]\n",
    "addlist=[1,1]\n",
    "while fib[-1]<4000000:\n",
    "    i=sum([i*j for (i, j) in zip(fib, addlist)])\n",
    "    fib.append(i)\n",
    "    addlist.insert(0,0)\n",
    "fib[-1]=0\n",
    "answer=sum([j for j in fib if j%2==0])\n",
    "print(answer)"
